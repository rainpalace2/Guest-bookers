<div class="container">
 <div class="row">

<div class="col-md-3">
 <%= render 'books/list', user: current_user %>
 <%= render 'books/book_new', book: @book %>
</div>

<div class="col-md-8 offset-md-1 pt-3">
 <h1>Users</h1>
 <table class="table">
  <thead>
   <tr>
   <th>image</th>
   <th>name</th>
   <th><% @user.books.each do |user| %></th>
   </tr>
 </thead>
 <% end %>
 <tbody>
  <tr>
   <td><%= image_tag current_user.get_profile_image(100,100) %></td>
   <td><%= current_user.name %></td>
   <td><%= link_to "Show",user_path(current_user.id) %></td>
  </tr>
 </tbody>
</table>
